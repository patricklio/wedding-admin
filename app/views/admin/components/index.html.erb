<%= render 'shared/sidebar'%>
<div class="main-content" style="min-height: 1812px;">
  <%= render 'shared/navbar' %>
  <hr>
  <!-- Layout for breadcrumb-->
  <ol class="breadcrumb bc-3">
    <li>
      <%= link_to root_path do %>
        <i class="material-icons md-18">home</i>Accueil
      <% end %>
    </li>
    <li>
      <%= link_to "Tableau de bord", root_path %>
    </li>
  </ol>

  <!-- Layout for page title with some actions-->
  <div class="row">
    <h2 class="col-md-8 col-sm-8 col-xs-8 list-title clearfix">Catégories de véhicules (2)</h2>

    <div class="col-md-4 col-sm-4 col-xs-4 clearfix">
      <%= link_to "" do %>
        <button type="button" class="btn btn-green btn-icon icon-left pull-right">
          Ajouter<i class="material-icons">add</i>
        </button>
      <% end %>
      <%= link_to "" do %>
        <button type="button" class="btn btn-info btn-icon icon-left pull-right" style="margin-right: 10px">
          Exporter<i class="material-icons">cloud_download</i>
        </button>
      <% end %>
    </div>
  </div>

  <br>

  <!-- Layout for datatable-->
  <%= render 'datatable' %>

    <br>
    <%= render 'admin/components/card_info_dashbord' %>
    <br>
    <H3>SIMPLE TABLE </H3>
    <%= render'admin/components/table_dashbord' %>
    <H3> SIMPLE FORM INSIDE COLAPSIBLE TAB</H3>
    <%= render 'admin/components/dashbord_form' %>
    <!-- Layout for footer -->
    <footer class="main">
      © 2020 Kulucar Admin
    </footer>
</div>
