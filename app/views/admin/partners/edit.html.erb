<%= render 'shared/sidebar'%>

<div class="main-content">
  <%= render 'shared/navbar' %>
  <hr>
  <!-- breadcrumb-->
  <ol class="breadcrumb bc-3">
    <li>
      <%= link_to root_path do %>
        <i class="material-icons md-18">home</i>Accueil
      <% end %>
    </li>
    <li>
      <%= link_to "Partenaires", admin_partners_path %>
    </li>
    <li>
      <strong> <%=  @partner.name  %> </strong>
    </li>
  </ol>

  <div class="row">
    <h2 class="col-md-8 col-sm-8 col-xs-8 list-title clearfix">
      Partenaire
    </h2>
  </div>

  <br>
  <%= render 'shared/flashes' %>
  <div class="row">
    <div class="col-md-12">
      <%= render  "form", partner: @partner,  methode: 'patch' %>
    </div>
  </div>

  <hr>
  <!-- Page Title-->
  <div class="row">
    <h2 class="col-md-8 col-sm-8 col-xs-8 list-title clearfix">
      Nouveau compte utilisateur
    </h2>
  </div>

  <br>

  <!-- Real content of the page-->
  <div class="row">
    <div class="col-md-12">
      <%= render  "user_account_form", partner: @partner, partner_user_account: @partner_account %>
    </div>
  </div>

  <%= render  "user_accounts" %>



  <!-- footer -->
  <footer class="main">
    Â© 2020 Kulucar Admin
  </footer>

</div>


